@model List<UserInfo>

<table style="width:100%;border-collapse:collapse;">
    <thead>
        <tr>
            <th style="border:1px solid #ccc;padding:8px;">Name</th>
            <th style="border:1px solid #ccc;padding:8px;">Tel</th>
            <th style="border:1px solid #ccc;padding:8px;">Email</th>
            <th style="border:1px solid #ccc;padding:8px;">Username</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td style="border:1px solid #ccc;padding:8px;">@user.Name</td>
                <td style="border:1px solid #ccc;padding:8px;">@user.Tel</td>
                <td style="border:1px solid #ccc;padding:8px;">@user.Email</td>
                <td style="border:1px solid #ccc;padding:8px;">@user.UserName</td>
            </tr>
        }
    </tbody>
</table>